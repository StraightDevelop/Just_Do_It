{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "types": ["node"],
    "baseUrl": "./"
  }
}
